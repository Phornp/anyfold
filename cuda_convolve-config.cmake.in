# - Config file for the FooBar package
# It defines the following variables
#  CUDA_CONVOLVE_INCLUDE_DIRS - include directories for Cuda_convolve
#  CUDA_CONVOLVE_LIBRARIES    - libraries to link against
#  CUDA_CONVOLVE_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(CUDA_CONVOLVE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(CUDA_CONVOLVE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${CUDA_CONVOLVE_CMAKE_DIR}/cuda_convolve-targets.cmake")

# These are IMPORTED targets created by cuda_convolve-targets.cmake
IF(UNIX)
  IF(APPLE)
    get_filename_component(CUDA_CONVOLVE_INCLUDE_DIRS "${CUDA_CONVOLVE_CMAKE_DIR}/../../include" ABSOLUTE)
    ELSE(APPLE)
    get_filename_component(CUDA_CONVOLVE_INCLUDE_DIRS "${CUDA_CONVOLVE_CMAKE_DIR}/../../../include" ABSOLUTE)
  ENDIF(APPLE)
ENDIF(UNIX)


set(CUDA_CONVOLVE_VERSION  "@CUDA_CONVOLVE_VERSION@")
set(CUDA_CONVOLVE_VERSION_MAJOR  "@CUDA_CONVOLVE_VERSION_MAJOR@")
set(CUDA_CONVOLVE_VERSION_MINOR  "@CUDA_CONVOLVE_VERSION_MINOR@")
set(CUDA_CONVOLVE_VERSION_PATCH  "@CUDA_CONVOLVE_VERSION_PATCH@")
set(CUDA_CONVOLVE_BUILD_TYPE  "@CMAKE_BUILD_TYPE@")
